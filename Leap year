def isLeapYear(year):
     If(year==0 and year!=100) or year==400:
        return True
     Else:
         return false
year=2012:
if(isLeapYear(year)):
     Print("{} is a leap year. ". format(year)):
Else:
     Print("is a not leap year.". format(year)):
